    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "nodemon": "^2.0.15"
  },
  "devDependencies": {
    "@h3aven-labs/h3-eslint": "2.0.0",
    "@smithy/types": "^4.3.2",
    "@types/express": "^5.0.5",
    "@types/jest": "^29.5.12",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/morgan": "^1.9.3",
    "@types/node": "^20.3.2",
    "@types/nodemon": "^1.19.1",
    "async_hooks": "^1.0.0",
    "esbuild": "^0.24.0",
    "fsevents": "^2.3.3",
    "jest": "^29.7.0",
    "prettier": "^2.4.1",
    "rimraf": "^6.0.1",
