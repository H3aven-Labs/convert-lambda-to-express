{
  "name": "@h3aven-labs/convert-lambda-to-express",
  "version": "1.7.1",
  "description": "Wrapper to run lambda on express. Works great for running lambdas as an express server during development but is production ready. Developed to work in conjunction with matthewkeil/full-stack-pattern cdk construct.",
  "scripts": {
    "node": "node",
    "ts": "npm run node -- -r ts-node/register",
    "test": "jest",
    "lint": "npx eslint --ext .ts",
    "lint:fix": "npx eslint . --ext .ts --fix",
    "build": "npx rimraf ./dist && npx tsc --project tsconfig.build.json && npx cross-env NODE_OPTIONS=--openssl-legacy-provider npx webpack",
    "postversion": "git push --tags",
    "clean-publish": "npm run build && npx clean-publish --fields \"scripts, dependencies\""
  },
  "dependencies": {
    "@aws-sdk/credential-providers": "^3.879.0",
    "chokidar": "^3.5.2",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "helmet": "^4.6.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "nodemon": "^2.0.15",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@smithy/types": "^4.3.2",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jest": "^29.5.12",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/morgan": "^1.9.3",
    "@types/node": "^20.3.2",
    "@types/nodemon": "^1.19.1",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "async_hooks": "^1.0.0",
    "clean-publish": "^5.2.2",
    "cross-env": "^10.0.0",
    "eslint": "^8.3.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-prettier": "^4.0.0",
    "fsevents": "^2.3.3",
    "jest": "^29.7.0",
    "prettier": "^2.4.1",
    "rimraf": "^6.0.1",
    "ts-jest": "^29.4.1",
    "ts-loader": "^9.5.4",
    "ts-node": "^10.4.0",
    "typescript": "^5.7.3",
    "webpack": "^5.101.3",
    "webpack-cli": "^6.0.1"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "license": "MIT",
  "author": "Matthew Keil",
  "maintainers": [
    {
      "name": "Matthew Keil (matthewkeil)",
      "email": "me@matthewkeil.com"
    }
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/H3aven-Labs/convert-lambda-to-express.git"
  },
  "bugs": {
    "url": "https://github.com/H3aven-Labs/convert-lambda-to-express/issues"
  },
  "keywords": [
    "lambda",
    "convert",
    "express",
    "aws-lambda",
    "developer-tools",
    "prevent lock-in",
    "lambda to express",
    "amazon",
    "aws",
    "local",
    "run"
  ],
  "packageManager": "yarn@4.9.2"
}
