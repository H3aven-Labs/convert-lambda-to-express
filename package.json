{
  "name": "run-lambda-as-express",
  "version": "1.0.0",
  "description": "Wrapper to run lambda on express.  Developed to work in conjunction with matthewkeil/full-stack-pattern cdk construct.  Works great for running lambdas as an express server during development but is production ready.",
  "main": "build/lambdalocal.js",
  "types": "build/lambdalocal.d.ts",
  "bin": {
    "lambda-local": "build/cli.js"
  },
  "scripts": {
    "node": "node",
    "ts": "npm run node -- -r ts-node/register",
    "test": "mocha",
    "build": "tsc"
  },
  "files": [
    "*.json",
    "build",
    "examples",
    "Makefile"
  ],
  "keywords": [
    "lambda",
    "amazon",
    "aws",
    "AWS",
    "local",
    "run"
  ],
  "author": "ashiina",
  "maintainers": [
    {
      "name": "Gabriel Potter (gpotter2)",
      "email": "gabriel@potter.fr"
    }
  ],
  "engines": {
    "node": ">=6"
  },
  "dependencies": {
    "aws-sdk": "^2.1017.0",
    "commander": "^7.2.0",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.84",
    "@types/express": "^4.17.13",
    "@types/node": "^15.12.4",
    "chai": "^4.3.4",
    "mocha": "^9.0.1",
    "sinon": "^11.1.1",
    "ts-node": "^10.4.0",
    "typescript": "^4.3.4"
  },
  "preferGlobal": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/ashiina/lambda-local.git"
  },
  "bugs": {
    "url": "https://github.com/ashiina/lambda-local/issues"
  },
  "license": "MIT"
}
